[user]
    name = sunny-owl
    email = sunny_hsiao@owlting.com
[color]
	diff = auto
	status = auto
	branch = auto
	log = auto
	ui = true
[push]
	default = simple
[alias]
	br = branch
	st = status
    co = checkout
    com = commit
[apply]
	whitespace = nowarn
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
